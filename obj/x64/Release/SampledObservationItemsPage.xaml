                       RelativePanel.AlignVerticalCenterWithPanel="True"
                       FontSize="18"
                       FontFamily="Segoe MDL Assets"
                       FontWeight="SemiBold"
                       Margin="10, 0, 0, 0" Foreground="White"/>
            <TextBlock x:ConnectionId='44' Name="LogIndex"
                       Text="N"
                       RelativePanel.RightOf="LogNoText"
                       RelativePanel.AlignVerticalCenterWithPanel="True"
                       FontSize="18"
                       FontFamily="Segoe MDL Assets"
                       FontWeight="SemiBold"
                       Foreground="White"
                       Margin="10, 0, 0, 0"/>
